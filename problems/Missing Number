class Solution {
    public int missingNumber(int[] nums) {
        int n=nums.length;
        int acctualsum=0;
        int sum=(n*(n+1))/2;
        for(int i=0;i<n;i++){
            acctualsum=acctualsum+nums[i];
        }
        int missing_number=sum-acctualsum;
      return missing_number; 
    }
}
